version: 0.32
compile:
  prune: true
  embed-refs: true
  fetch: true
  yaml-dump-null-as-empty: true
  compose-node-name: true
  refs-path: ./system/refs
  jinja2-filters: ./system/lib/jinja2_filters.py
  search-paths:
    - .
    - ./system/
    - ./system/lib
    - ./system/generators
refs:
  refs-path: ./system/refs